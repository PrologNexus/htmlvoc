@prefix doc: <http://www.example.org/document/> .
@prefix html: <https://data.rijksfinancien.nl/html/model/def/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

doc:1 a html:Document ;
    rdf:_1 doc:1.15 .

doc:1.15 a html:Html ;
    rdf:_1 doc:1.21 ;
    rdf:_2 doc:1.1113 .

doc:1.21 a html:Head ;
    rdf:_1 doc:1.27 ;
    rdf:_2 doc:1.42 ;
    rdf:_3 doc:1.1058 .

doc:1.27 a html:Title ;
    rdf:_1 doc:1.271.

doc:1.271 a html:TextElement;
    html:fragment "My first 'Hello world' HTML page represented in RDF" .

doc:1.42 a html:StyleSheet ;
    rdf:_1 doc:1.42.1 .

doc:1.42.1 a html:TextElement ;
    html:fragment " .italic-text {font-style: italic;}" .

doc:1.1058 a html:Meta ;
    html:charset "utf-8" ;
    html:http-equiv "Content-Type" .

doc:1.1113 a html:Body ;
    rdf:_1 doc:1.1119 .

doc:1.1119 a html:TextElement ;
    html:class 'italic-text';
    html:fragment "Hello world" .

